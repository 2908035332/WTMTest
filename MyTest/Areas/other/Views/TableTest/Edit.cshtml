@model MyTest.ViewModel.other.TableTestVMs.TableTestVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:textbox field="Entity.Name" placeholder="请输入姓名"/>
        <wt:radio field="Entity.Sex" padding-text="请选择性别"/>
        <wt:datetime field="Entity.Birthday" placeholder="请输入生日"/>
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
