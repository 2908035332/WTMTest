@model MyTest.ViewModel.other.ExamVMs.ExamVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
	<wt:quote>@Model.Localizer["DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Exam_Name" />
<wt:display field="Entity.Exam_A" />
<wt:display field="Entity.Exam_B" />
<wt:display field="Entity.Exam_C" />
<wt:display field="Entity.Exam_D" />
<wt:display field="Entity.Exam_Type.TypeName" />
<wt:display field="Entity.TrueEnum" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
