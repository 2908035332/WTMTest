@model MyTest.ViewModel.other.ExamVMs.ExamVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.Exam_Name" />
<wt:textbox field="Entity.Exam_A" />
<wt:textbox field="Entity.Exam_B" />
<wt:textbox field="Entity.Exam_C" />
<wt:textbox field="Entity.Exam_D" />
<wt:combobox field="Entity.Exam_TypeId" items="AllExam_Types"/>
<wt:combobox field="Entity.TrueEnum" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
